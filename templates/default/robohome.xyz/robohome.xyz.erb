server {
	listen 80 default_server;
    listen [::]:80 default_server;
	# listen 443 ssl default_server;
	# listen [::]:443 ssl default_server;

	# include snippets/ssl-params-robohome.xyz.conf;

	# ssl_certificate /etc/letsencrypt/live/robohome.xyz/fullchain.pem;
	# ssl_certificate_key /etc/letsencrypt/live/robohome.xyz/privkey.pem;

	root /var/www/robohome.xyz/public;

	index index.php index.html;

	server_name robohome.xyz www.robohome.xyz;

	location / {
		try_files $uri $uri/ /index.php?$query_string;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/run/php/php7.0-fpm.sock;
	}

	location ~ /\.ht {
		deny all;
	}

	location ~ /.well-known {
		allow all;
	}
}